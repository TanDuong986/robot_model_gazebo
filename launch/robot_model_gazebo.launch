<?xml version="1.0"?>
<!-- How to run: roslaunch mybot_gazebo mybot_gazebo.launch model:="`rospack find mybot_description`/urdf/mybot.xacro"
-->
<launch>
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
    		<arg name="use_sim_time" value="true"/>
    		<arg name="debug" value="false"/>
			<arg name="gui" value="true"/>
		<arg name="world_name" value="$(find robot_model_gazebo)/worlds/complete.world"/>
  	</include>
  	
  	  <!-- <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="log">
    	  <param name="base_frame" value="base_link"/>
          <param name="odom_frame" value="odom"/>
          <param name="map_frame"  value="map"/>
          <rosparam command="load" file="$(find deliver_rb)/config/gmapping_params.yaml" />
          </node> -->

		<!-- slam_karto -->
  <node pkg="slam_karto" type="slam_karto" name="slam_karto" output="screen">
    <rosparam command="load" file="$(find robot_model_gazebo)/config/karto_mapper_params.yaml" />
  </node>

	<!--arg name="model" /-->
	<param name="robot_description" command="$(find xacro)/xacro '$(find robot_model_gazebo)/urdf/robot_model_gazebo.xacro'" />
	<node pkg="joint_state_publisher" name="joint_state_publisher"  type="joint_state_publisher" ></node>
	<!--node pkg="robot_state_publisher" name="robot_state_publisher"  type="state_publisher" output="screen" -->
 	<node pkg="robot_state_publisher" name="robot_state_publisher"  type="robot_state_publisher" output="screen" >
		<param name="publish_frequency" type="double" value="20.0" />
	</node>
	<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -z 0.1 -model robot_model" respawn="false" output="screen"/>
	<!-- -->
	<node name="rviz" pkg="rviz" type="rviz" output = "screen" args="-d $(find robot_model_gazebo)/robot_model_gazebo.rviz" /> 
	
</launch>

